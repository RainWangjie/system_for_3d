<div class="col-md-12">
    <select class="model-type">
        {{#each styleList}}
            <option value="{{styleNum}}">{{styleName}}</option>
        {{/each}}
    </select>
</div>
<div class="col-md-12 model-list">

</div>
<script id="tpl" type="text/html">
    <%for(var i in model){
    var item = model[i];%>
    <div class="col-md-4">
        <div class="panel panel-default">
            <a href="/admin/webPreview/<%=item.id%>" class="model-preview-img">
                <img src="<%=item.previewImg%>?imageView2/2/w/500" width="100%">
            </a>
            <div>
                <span>模型名称：<%=item.name%></span>
            </div>
            <div class="btn-group">
                <a href="/admin/webPreview/<%=item.id%>" class="btn btn-sm btn-success model-audit-edit">编辑</a>
                <button class="btn btn-sm btn-danger model-audit-failed" data-id="<%=item.id%>">下架</button>
            </div>
        </div>
    </div>
    <%}%>
</script>